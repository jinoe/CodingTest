    "# 04\n",
    "x, k = map(int, input().split())\n",
    "count = 0\n",
    "while(1):\n",
    "    if x == 1:\n",
    "        break\n",
    "    if x % k == 0:\n",
    "        x //= k\n",
    "        # print(x)\n",
    "    else:\n",
    "        x -= 1\n",
    "        # print(x)\n",
    "    count += 1\n",
    "\n",
    "print(count)\n",
    "    "
   ]
